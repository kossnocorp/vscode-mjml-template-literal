{
  "name": "vscode-mjml-template-literal",
  "displayName": "vscode-mjml-template-literal",
  "description": "Syntax highlighting for MJML in template literals",
  "version": "0.1.0",
  "license": "MIT",
  "publisher": "kossnocorp",
  "repository": {
    "type": "git",
    "url": "https://github.com/kossnocorp/vscode-mjml-template-literal.git"
  },
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "grammars": [
      {
        "injectTo": [
          "source.js",
          "source.js.jsx",
          "source.jsx",
          "source.ts",
          "source.tsx"
        ],
        "scopeName": "inline.mjml-template",
        "path": "./syntaxes/lit-mjml.json",
        "embeddedLanguages": {
          "meta.embedded.block.mjml": "mjml"
        }
      }
    ]
  }
}
